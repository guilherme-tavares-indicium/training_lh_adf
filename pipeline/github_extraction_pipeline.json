{
	"name": "github_extraction_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Extract",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"User-Agent": "guilherme.tavares"
						},
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "RestGithub_source",
						"type": "DatasetReference",
						"parameters": {
							"per_page": "1000"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "blobstorage_csv_target",
						"type": "DatasetReference",
						"parameters": {
							"filename": {
								"value": "github_issues_@{formatDateTime(utcnow(), 'yyyy-dd-MM')}.csv",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "Load",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Extract",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"modifiedDatetimeStart": {
								"value": "@adddays(utcnow(),-1)",
								"type": "Expression"
							},
							"wildcardFolderPath": "github",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzurePostgreSQLSink",
						"writeBatchSize": 1000000,
						"writeBatchTimeout": "00:30:00",
						"preCopyScript": "truncate table target.issues;",
						"writeMethod": "CopyCommand"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "blobstorage_csv_source",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "PostgreSql_target",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"debug_output": {
				"type": "String"
			}
		},
		"annotations": [],
		"lastPublishTime": "2023-05-30T17:49:38Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}